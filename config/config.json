        }
      ]
    },
    "sap-todas-as-tarefas-adm-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/sapTodasAsTarefasAdm/SapTodasAsTarefasAdmWebPart.js",
          "manifest": "./src/webparts/sapTodasAsTarefasAdm/SapTodasAsTarefasAdmWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "SapNovaPropostaWebPartStrings": "lib/webparts/sapNovaProposta/loc/{locale}.js",
    "SapDetalhesPropostaWebPartStrings": "lib/webparts/sapDetalhesProposta/loc/{locale}.js",
    "SapTodasPropostasWebPartStrings": "lib/webparts/sapTodasPropostas/loc/{locale}.js",
    "SapEditarPropostaWebPartStrings": "lib/webparts/sapEditarProposta/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "SapTodasAsTarefasWebPartStrings": "lib/webparts/sapTodasAsTarefas/loc/{locale}.js",
    "SapTodasAsTarefasAdmWebPartStrings": "lib/webparts/sapTodasAsTarefasAdm/loc/{locale}.js"
  }
}
